<MenuScreen>:
	name: 'menu'
	NavigationLayout:
		ScreenManager:
			Screen:
				BoxLayout:
					orientation: 'vertical'			  
					MDToolbar:
						title: 'Text Material'
						type: 'top'
						left_action_items: [['menu',lambda x: nav_drawer.toggle_nav_drawer()]]
						elevation:5
					BoxLayout :
						orientation: 'vertical'				
						ScrollView:
							GridLayout:
								cols: 1
								
								spacing: root.height/3
								size_hint_y: None
								height:self.minimum_height
								MDList:
									TwoLineAvatarIconListItem:
										text:"Python"
										font_style: 'H6'
										secondary_text:'Introduction'
										on_release:
											root.manager.current="profile1"
										ImageLeftWidget:
											source:'alphaimg/music.ico'		
									TwoLineAvatarIconListItem:
										text:"Flow of Control"
										font_style: 'H6'
										secondary_text:"understanding algorithm"
										on_release:
											root.manager.current="profile3"
										ImageLeftWidget:
											source:'alphaimg/music.ico'
									
									TwoLineAvatarIconListItem:
										text:"Datatypes"
										font_style: 'H6'
										secondary_text:'datatypes in python'
										on_release:
											root.manager.current="profile5"
										ImageLeftWidget:
											source:'alphaimg/music.ico'													
										
									TwoLineAvatarIconListItem:
										text:"Operators"
										font_style: 'H6'
										secondary_text:'basic operators in python'
										on_release:
											root.manager.current="profile4"
										ImageLeftWidget:
											source:'alphaimg/music.ico'												
										
																				
									